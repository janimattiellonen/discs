{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier",
        "plugin:react-hooks/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "func-names": "off",
        "import/prefer-default-export": "off",
        "no-console": "warn",
        "no-unused-vars": "warn",
        "react/jsx-props-no-spreading": "off",
        "react/prop-types": "off"
    },
    "ignorePatterns": ["temp.js", "**/test/*.js", "**/*.test.js"]
}
